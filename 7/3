per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = input('money: ')
#print(list(per_cent.values()))
dep = list(per_cent.values())
dep_a = int(float(dep[0])*int(money)/100)
dep_b = int(float(dep[1])*int(money)/100)
dep_c = int(float(dep[2])*int(money)/100)
dep_d = int(float(dep[3])*int(money)/100)
deposit = [dep_a, dep_b, dep_c, dep_d]
print('deposit =', deposit)
print('Максимальная сумма, которую вы можете заработать -', max(deposit))
